{
    "version": 2,
    "builds": [
        {
            "src": "django_notes_app/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLamdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "django_notes_app/wsgi.py"
        },
        {
            "src": "/react-notes-app/build/(.*)",
            "dest": "react-notes-app/build/$1"
        }
    ]
}